&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    &MGRID
       CUTOFF 400
       NGRIDS 5
    &END
    &QS
      LS_SCF
    &END QS
    &LS_SCF
      MAX_SCF     25
      EPS_SCF     1.0E-8
      EPS_FILTER  1.0E-12
      S_PRECONDITIONER  NONE
      REPORT_ALL_SPARSITIES OFF
      PURIFICATION_METHOD TRS4
      EXTRAPOLATION_ORDER  1
      &PAO
        PREOPT_DM_FILE  64H2O_ref_LS_DM_SPIN_1_RESTART.dm
        EPS_PAO         5.0E-7
        MAX_PAO         1500
        PARAMETERIZATION EQUIVARIANT
        &PRINT
          &RESTART ON
            BACKUP_COPIES 0
          &END RESTART
        &END PRINT
      &END PAO
    &END
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
     ABC 12.4544808085 12.4544808085 12.4544808085
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME coords.xyz
      COORD_FILE_FORMAT XYZ
    &END TOPOLOGY
    &KIND H
      BASIS_SET DZVP-MOLOPT-GTH
      POTENTIAL GTH-PBE
      PAO_BASIS_SIZE 4
    &END KIND
    &KIND O
      BASIS_SET DZVP-MOLOPT-GTH
      POTENTIAL GTH-PBE
      PAO_BASIS_SIZE 4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT 64H2O_pao44
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
